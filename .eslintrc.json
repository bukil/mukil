{
  "root": true,
  "extends": ["next", "next/core-web-vitals"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrors": "all",
        "caughtErrorsIgnorePattern": "^_"
      }
    ],
    "@next/next/no-page-custom-font": "off",
    "react/display-name": 0
  }
}
